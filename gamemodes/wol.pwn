stock OnPlayerLogin(playerid, password[])
{
	new my_query[144],sendername[24];
	GetPlayerName(playerid,sendername,24);
	mysql_format(connects,my_query, sizeof(my_query),"SELECT * FROM `accounts` WHERE `Chelovek` = '%e' AND `pKey` = '%e'", sendername, MD5_Hash(password));
	mysql_tquery(connects, my_query, "LoginCallback","ds", playerid, password);
	return true;
}
publics: OffWarn(playerid,tmp[])
{
	new rows, fields,my_query[128];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	format(my_query, sizeof(my_query), "UPDATE `accounts` SET `pWarns` = '1', `pLRInections` = '0', `pMRInections` = '0', `pRKInections` = '0' WHERE `Chelovek` = '%s'", tmp);
	mysql_tquery(connects, my_query, "", "");
//	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно был предупреждён", tmp);
	format(stringer, sizeof(stringer), "Игрок %s получил оффлайн warn от Администратора %s",tmp, PlayerInfo[playerid][pNames]);
	ABroadCast(COLOR_LIGHTRED,stringer,1);
	return true;
}

publics: Delete(playerid,tmp[])
{
	new rows, fields,my_query[128];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	format(my_query,sizeof(my_query),"DELETE FROM `accounts` WHERE `Chelovek` = '%s'",tmp);
	mysql_tquery(connects, my_query, "", "");
	SendMes(playerid,COLOR_YELLOW,"Аккаунт с именем %s был успешно удален!",tmp);
	return true;
}

publics: UnBan(playerid,tmp[])
{
	new rows, fields,tmp1[2],my_query[150];
	cache_get_data(rows, fields);
	if(!rows) return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.",tmp);
	cache_get_field_content(0, "pBanKol", tmp1);
	format(my_query, sizeof(my_query), "UPDATE `accounts` SET `pLocked2` = '0',`pBanData1` = '0', `pBanTime1` = '0', `pBanKol` = '%d' WHERE `Chelovek` = '%s'",strval(tmp)-1,tmp);
	mysql_tquery(connects, my_query, "", "");
	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно разбанен", tmp);
	format(stringer, sizeof(stringer), "[UNBAN] {FFFFFF}Администратор {FF6347}%s {FFFFFF}разбанил {FF6347}%s",PlayerInfo[playerid][pNames],tmp);
	ABroadCast(COLOR_LIGHTRED,stringer,1);
	return true;
}

