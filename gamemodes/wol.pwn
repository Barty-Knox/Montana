stock LoadGZ()
{
	new File = ini_openFile("richzone.ini");
	new l[128];
	//new stringer[64];
	new asd[5][10];
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		format(stringer,sizeof(stringer),"Zone%d",i);
		new x = ini_getString(File,stringer,l);
		if(x < 0)
		{
			GZInfo[i][gCoords][0] = STANDARTGangZone[i][0];
			GZInfo[i][gCoords][1] = STANDARTGangZone[i][1];
			GZInfo[i][gCoords][2] = STANDARTGangZone[i][2];
			GZInfo[i][gCoords][3] = STANDARTGangZone[i][3];
		}
		else
		{
			split(l,asd,'|');
			GZInfo[i][gCoords][0] = floatstr(asd[0]);
			GZInfo[i][gCoords][1] = floatstr(asd[1]);
			GZInfo[i][gCoords][2] = floatstr(asd[2]);
			GZInfo[i][gCoords][3] = floatstr(asd[3]);
		}
		GZInfo[i][gFrakVlad] = strval(asd[4]);
		if(GZInfo[i][gFrakVlad] == 0) GZInfo[i][gFrakVlad] = allowedfactions[random(sizeof(allowedfactions))];
		GZInfo[i][gID] = GangZoneCreate(GZInfo[i][gCoords][0],GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]);
		ZoneOnBattle[i] = 0;
	}
	ini_closeFile(File);
}
stock SaveGZ()
{
	new File = ini_openFile("richzone.ini");
	new l[64];
	//new stringer[144];
	for(new i =0;i<sizeof(GZInfo);i++)
	{
		format(l,sizeof(l),"Zone%d",i);
		format(stringer,sizeof(stringer),"%f|%f|%f|%f|%d",GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3],GZInfo[i][gFrakVlad]);
		ini_setString(File,l,stringer);
	}
	ini_closeFile(File);
}
stock GzCheck()
{
	if(capt == 1 || capt == 10)//capture[12] == 1 || capture[13] == 1 || capture[15] == 1 || capture[17] == 1 || capture[18] == 1)
	{
	    if(WarZoneID == 255) return true;
	    WarTimeSec--;
	    if(WarTimeSec > 1 && WarTimeMin >= 0 || WarTimeMin > 0)
	    {
			if(WarTimeSec == 0 && WarTimeMin != 0) { WarTimeSec = 60; WarTimeMin--; }
			new stringtime[32];
			if(WarTimeSec > 9) format(stringtime, sizeof(stringtime),"  %d:%d",WarTimeMin,WarTimeSec);
			else format(stringtime, sizeof(stringtime),"  %d:0%d",WarTimeMin,WarTimeSec);
			TextDrawSetString(vremya, stringtime);
		}
		else
		{
			foreach(Player, z)
			{
		 		if(PlayerInfo[z][pMRInections] == 12 || PlayerInfo[z][pMRInections] == 13 || PlayerInfo[z][pMRInections] == 15 || PlayerInfo[z][pMRInections] == 17 || PlayerInfo[z][pMRInections] == 18)
		 		{
					TextDrawHideForPlayer(z, fon);
					TextDrawHideForPlayer(z, onecommand);
					TextDrawHideForPlayer(z, twocommand);
					TextDrawHideForPlayer(z, kolone);
					TextDrawHideForPlayer(z, koltwo);
					TextDrawHideForPlayer(z, timetext);
					TextDrawHideForPlayer(z, vremya);
				}
			}
			GangZoneStopFlashForAll(WarZoneID);
			GangZoneHideForAll(WarZoneID);
			if(CommandKill[1] > CommandKill[0])
			{
				SendFamilyMessage(captureteam[1],COLOR_YELLOW,"[F] Вам удалось отстоять свою территорию");
				SendFamilyMessage(captureteam[0],COLOR_RED,"[F] Вам не удалось завоевать территорию");
				switch(GZInfo[WarZoneID][gFrakVlad])
				{
					case 12: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xd200ff99);
					case 18: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x007fffAA);
					case 13: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xcfbe0899);
					case 15: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x06b50699);
					case 17: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x03c3b899);
					default: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xFFBE0065);
				}
				FrakCD[GZInfo[WarZoneID][gNapad]] = 0;
				FrakCD[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gNapad]] = 0;
				GZInfo[WarZoneID][gNapad] = 0;
			}
			else
			{
				SendFamilyMessage(captureteam[1],COLOR_RED,"[F] Вы потеряли контроль над одной из своих территорий");
				SendFamilyMessage(captureteam[0],COLOR_YELLOW,"[F] Вы приобрели контроль над новой территорией");
				switch(GZInfo[WarZoneID][gNapad])
				{
					case 12: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xd200ff99);
					case 18: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x007fffAA);
					case 13: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xcfbe0899);
					case 15: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x06b50699);
					case 17: GangZoneShowForAll(GZInfo[WarZoneID][gID],0x03c3b899);
					default: GangZoneShowForAll(GZInfo[WarZoneID][gID],0xFFBE0065);
				}
				FrakCD[GZInfo[WarZoneID][gNapad]] = 0;
				FrakCD[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gFrakVlad]] = 0;
				ScoreZahvat[GZInfo[WarZoneID][gNapad]] = 0;
				GZInfo[WarZoneID][gFrakVlad] = GZInfo[WarZoneID][gNapad];
				GZInfo[WarZoneID][gNapad] = 0;
			}
			captureteam[0] = 0;
			captureteam[1] = 0;
			WarTimeSec = 0;
			if(capt == 1) capt = 2;
			else if(capt == 10) capt = 4;
			WarZoneID = 255;
			SaveGZ();
		}
	}
	return true;
}
