	}
	new vehicleID = GetPlayerVehicleID(playerid);
	if(IsPlayerInVehicle(playerid,vehicleID) && (GetPlayerWeapon(playerid) == WEAPON_DEAGLE || GetPlayerWeapon(playerid) == WEAPON_SHOTGSPA || GetPlayerWeapon(playerid) == WEAPON_SAWEDOFF || GetPlayerWeapon(playerid) == WEAPON_MINIGUN || GetPlayerWeapon(playerid) == WEAPON_MP5 )) SetPlayerArmedWeapon(playerid,0);
	if(!GetPVarInt(playerid, "NoAB"))
