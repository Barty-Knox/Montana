public OnPlayerDeath(playerid, killerid, reason)
{
    plafk[playerid] = 0;

	new str[128];

    if (GetTickCount() - DeathTime[playerid] < 1000)
	{
		new ip[24];
		GetPlayerIp(playerid, ip, 24);
		format(str, 128, "[ICU]: %s[%i] (%s) возможно Death Flood", PlayerInfo[playerid][pNames], playerid, ip);
		ABroadCast(COLOR_REDD, str, 1);
		Kick(playerid);

		return 1;
	} else
		DeathTime[playerid] = GetTickCount();

	newAFKill[playerid] ++;
    if(killerid != INVALID_PLAYER_ID)
	{
		newAFKill[killerid] ++;

	    if (newAFKill[killerid] > 2)
		{

			new ip[24];
			GetPlayerIp(killerid, ip, 24);
			format(str, 128, "[ICU]: %s[%i] (%s) возможно Rapid | Fake Kill", PlayerInfo[killerid][pNames], killerid, ip);
			ABroadCast(COLOR_REDD, str, 1);
			newAFKill[killerid] = 0;
			Kick(killerid);
		}
	}
	
	if(!AntiFakeKill[playerid]) return Kick(playerid); // если ложь то кикаем
	AntiFakeKill[playerid] = false;
	SPD(playerid,65535,DIALOG_STYLE_MSGBOX,"1","1","","");
	SetPVarInt(playerid,"K_Times",GetPVarInt(playerid,"K_Times") + 1);
	if(GetPVarInt(playerid,"K_Times") > 1) return KickEx(playerid);
	//==============================================================================
	TeleportTime[playerid] = 3;
	//==============================================================================
	if(capt == 1 || capt == 10)
	{
		if(FrakCD[PlayerInfo[playerid][pMRInections]])
		{
			for(new i = 0;i<130;i++)
			{
				if(PlayerToKvadrat(playerid,GZInfo[i][gCoords][0], GZInfo[i][gCoords][1],GZInfo[i][gCoords][2],GZInfo[i][gCoords][3]))
				{
					if(captureteam[0] == PlayerInfo[killerid][pMRInections] && PlayerInfo[playerid][pMRInections] == captureteam[1])
					{
					    new fragkolv[8];
						CommandKill[0]++;
						format(fragkolv, sizeof(fragkolv), "%d", CommandKill[0]);
					 	TextDrawSetString(koltwo, fragkolv);
					}
					else if(captureteam[1] == PlayerInfo[killerid][pMRInections] && PlayerInfo[playerid][pMRInections] == captureteam[0])
					{
					    new fragkolv[8];
						CommandKill[1]++;
						format(fragkolv, sizeof(fragkolv), "%d", CommandKill[1]);
					 	TextDrawSetString(kolone, fragkolv);
					}
					else return true;
				}//сюдажек
			}
		}
	}
	
	if ((FrakCD[PlayerInfo[killerid][pMRInections]] == 1) && (FrakCD[PlayerInfo[playerid][pMRInections]] == 1) )//если они воюют
	{

		foreach(Player,i)
		{
			if(!IsPlayerConnected(i)) continue;

			if (PlayerInfo[i][pMRInections] == PlayerInfo[killerid][pMRInections] || PlayerInfo[i][pMRInections] == PlayerInfo[playerid][pMRInections])
				SendDeathMessageToPlayer(i, killerid, playerid, reason);
		}
	}

	if(PlayerInfo[playerid][pJob] == 2)
	{
		if(gcontract[playerid] == 1)
		{
			Delete3DTextLabel(Meh3d[GetPlayerVehicleID(playerid)]);
			gcontract[playerid] = 0;
		}
	}
	if(AlhambraDJ == true && AlhambraDJID == playerid) AlhambraDJ = false, AlhambraDJID = INVALID_PLAYER_ID;
	if(JizzyDJ == true && JizzyDJID == playerid) JizzyDJ = false, JizzyDJID = INVALID_PLAYER_ID;
