stock SaveAvto(houseid)
{
	new update[600];

	mysql_format(connects, update, 600, "UPDATE `avtomast` SET `abOwned` = %i, `abOwner` = %i, `abMessage` = %i, `aExitX` = %i, `aExitY` = %i, `aExitZ` = %i, `abBuyPrice` = %i, `abWorld` = %i, `abLocked` = %i, `abInterior` = %i, `abPrice` = %i, `abArenda` = %i, `abTill` = %i, `abZp` = %i, `Rab` = %i, `Poset` = %i, `Mesto` = %i WHERE `aID` = '%d'",
	AvtoInfo[houseid][abOwned], AvtoInfo[houseid][abOwner], AvtoInfo[houseid][abMessage], AvtoInfo[houseid][aExitX], AvtoInfo[houseid][aExitY], AvtoInfo[houseid][aExitZ], AvtoInfo[houseid][abBuyPrice], AvtoInfo[houseid][abWorld], AvtoInfo[houseid][abLocked], AvtoInfo[houseid][abInterior], AvtoInfo[houseid][abPrice], AvtoInfo[houseid][abArenda],
	AvtoInfo[houseid][abTill], AvtoInfo[houseid][abZp], AvtoInfo[houseid][Rab], AvtoInfo[houseid][Poset], AvtoInfo[houseid][Mesto], houseid);

	mysql_tquery(connects, update, "", "");
}
