				if(!strlen(tmp)) return SendClientMessage(playerid, COLOR_WHITE, ""#information"/tipster SET [playerid]");
				ammo = strval(tmp);
				if(!tipsterq[playerid]) return SendClientMessage(playerid,COLOR_ISPOLZUY,"У вас нет жучка");
				if(!ProxDetectorS(2.0, playerid, ammo)) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Игрок далеко от вас");
				if(tipsterw != -1) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Жучок уже прикреплен! Используйте /tipster REMOVE");
				if(!PlayerInfo[ammo][pMRInections]) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Игрок не состоит в организации");
				tipsterq[playerid] = 0;
				tipsterw = PlayerInfo[ammo][pMRInections];
				tipstere = ammo;
				format(stringer,sizeof(stringer),"%s прикрепил жучок к игроку %s. Используйте /tipster LISTEN для прослушивания",sendername ,giveplayer,ammo);
				SendRadioMessage(2,COLOR_ISPOLZUY, stringer);
				SendClientMessage(playerid,COLOR_ISPOLZUY,"Вы прикрепили жучок к игроку! Используйте /tipster LISTEN");
			}
			else if(strcmp(x_job, "listen",true) == 0)
			{
				if(tipsterw == -1) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Жучок не прикреплен!");
				if(!tipsterl[playerid])
				{
					tipsterl[playerid] = 1;
					SendClientMessage(playerid,COLOR_ISPOLZUY,"Вы включили прослушивание!");
				}
				else
				{
					tipsterl[playerid] = 0;
					SendClientMessage(playerid,COLOR_ISPOLZUY,"Вы отключили прослушивание!");
				}
			}
			else if(strcmp(x_job, "remove",true) == 0)
			{
				if(PlayerInfo[playerid][pRKInections] < 6) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Вам недоступна данная функция!");
