			return false;
		}
		format(stringer,sizeof(stringer),"[Античит]: %s получил Warn за использование читов",PlayerInfo[playerid][pNames]);
		SendClientMessageToAll(COLOR_LIGHTRED, stringer);
		PlayerInfo[playerid][pMRInections] = 0;
		PlayerInfo[playerid][pLRInections] = 0;
		KickEx(playerid);
	}
	if(!IsAPlane(playerid))
	{
		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
			if(SpeedVehicle(playerid) >= 230 && GetVehicleModel(GetPlayerVehicleID(playerid)) != 520)
