		new string[48];
		format(string,48,"%s был убит за Драйв Бай",PlayerInfo[killerid][pNames]);
		SendAdminMessage(COLOR_ORANGE, stringer);
		SendClientMessage(playerid, 0x0000C6FF,"Тот кто вас DB, был наказан");
