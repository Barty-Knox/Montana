	{
		minigun[playerid]+=2;
		new model = GetVehicleModel(GetPlayerVehicleID(playerid));
		if(400 <= model <= 611 && model != 483)
		{
			model -= 400;
			new seat = GetPlayerVehicleSeat(playerid);
			if(seat != 128)
			{
				model = (MaxPassengers[model >>> 3] >>> ((model & 7) << 2)) & 0xF;
