				tipsterq[playerid] = 1;
				SendClientMessage(playerid,COLOR_ISPOLZUY,"Вы взяли жучок! Используйте /tipster SET [id].");
				format(stringer,sizeof(stringer),"%s взял жучок из автомобиля",sendername ,giveplayer);
				SendRadioMessage(2,COLOR_ISPOLZUY, stringer);
			}
			else if(strcmp(x_job, "set",true) == 0)
			{
				new ammo, sendername[MAX_PLAYER_NAME], giveplayer[24];
				if(PlayerInfo[playerid][pRKInections] < 6) return SendClientMessage(playerid,COLOR_ISPOLZUY,"Вам недоступна данная функция!");
