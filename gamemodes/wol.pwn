	for(new pl = 0; pl < GetMaxPlayers(); pl++)
        {
            if(GetPVarInt(pl, "hide") == 1) ShowPlayerNameTagForPlayer(playerid, pl, false);
            else if(GetPVarInt(pl, "hide") == 0) ShowPlayerNameTagForPlayer(playerid, pl, true);
        }
 	if(GetPVarInt(playerid, "spectate_samp"))
    {
        DeletePVar(playerid, "spectate_samp");
        SetPlayerPos(playerid, GetPVarFloat(playerid, "PosSpecX"), GetPVarFloat(playerid, "PosSpecY"), GetPVarFloat(playerid, "PosSpecZ"));
        SetPlayerInterior(playerid, GetPVarInt(playerid, "SpecInt"));
        SetPlayerVirtualWorld(playerid, GetPVarInt(playerid, "SpecVT"));
        return true;
    }
	if(Maskaa[playerid] == 1)
	{
	    Maskaa[playerid] = 0;
	    KillTimer(UnMask(playerid));
	}
	return true;
