		if(POS[0] >= gZoneInfo[i][SAZONE_AREA][0] && POS[0] <= gZoneInfo[i][SAZONE_AREA][3] && POS[1] >= gZoneInfo[i][SAZONE_AREA][1] && POS[1] <= gZoneInfo[i][SAZONE_AREA][4])
		{
			settext(zone, gZoneInfo[i][SAZONE_NAME]);
			getzone = true;
			break;
		}
	}
	if(!getzone) zone = "Неизвестно";
	return zone;
}
