	{
		//new warnn[128];
		//format(warnn, sizeof(warnn), "<Warning> %s[%d]: убил с минигана %s[%d] wep %d guns %d",PlayerInfo[killerid][pNames],killerid,PlayerInfo[playerid][pNames],playerid,GetPlayerWeapon(killerid),GetPlayerAmmo(killerid));
